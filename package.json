{
  "name": "robotjs_jm",
  "version": "0.6.1",
  "description": "Node.js Desktop Automation. Forked for JM Robotics",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "run-script-os",
    "test:darwin:linux": "jasmine test/**/*.js",
    "test-keyboard": "node test/keyboard.js",
    "test:win32": "jasmine test/**/*.js",
    "install": "prebuild-install || node-gyp rebuild --target=20.1.4 --arch=x64 --dist-url=https://electronjs.org/headers",
    "install-debug": "prebuild-install || node-gyp rebuild --target=20.1.4 --arch=x64 --dist-url=https://electronjs.org/headers --debug",
    "prebuild": "prebuild --all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octalmage/robotjs.git"
  },
  "keywords": [
    "Automation",
    "GUI",
    "mouse",
    "keyboard",
    "screenshot",
    "image",
    "pixel",
    "desktop",
    "robotjs",
    "screen",
    "recognition",
    "autohotkey",
    "machine",
    "learning",
    "color"
  ],
  "author": "Jason Stallings, mads@jmrobotics.no",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/maphstr/robotjs_jm/issues"
  },
  "homepage": "https://github.com/maphstr/robotjs_jm",
  "dependencies": {
    "nan": "^2.16.0",
    "prebuild-install": "^5.3.3"
  },
  "devDependencies": {
    "tape": "^4.8.0",
    "jasmine": "^2.99.0",
    "prebuild": "^9.1.1",
    "run-script-os": "^1.0.3",
    "targetpractice": "0.0.7"
  }
}
